         console.log(error);\r\n          });\r\n      }\r\n    } else {\r\n      addToast(\"Fill all the mandatory fields *\", {\r\n        appearance: \"warning\",\r\n        autoDismiss: true,\r\n      });\r\n    }\r\n  };\r\n\r\n  const handleDelete = (e, id) => {\r\n    e.preventDefault();\r\n    axios\r\n      .delete(`${axios.defaults.baseURL}auth/delete-user/${id}`)\r\n      .then((response) => {\r\n        addToast(\"User deleted successfully\", {\r\n          appearance: \"success\",\r\n          autoDismiss: true,\r\n        });\r\n        set_refetch(!refetch);\r\n        props.history.push(`/web/user`);\r\n      })\r\n      .catch((error) => {\r\n        console.log(error);\r\n      });\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <div className=\"container-fluid\">\r\n        <h3 className=\"border-bottom\">User</h3>\r\n        <div className=\"row\">\r\n          <div className=\"col-4\">\r\n            <Link to=\"/web/user/new\" className=\"btn btn-dark btn-sm mb-2\">\r\n              Add new user\r\n            </Link>\r\n            <table className=\"table table-hover table-sm\">\r\n              <thead>\r\n                <tr>\r\n                  <th>Name</th>\r\n                  <th>Role</th>\r\n                  <th>Active</th>\r\n                  <th>Delete</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                {user_list.map((obj, i) => (\r\n                  <tr\r\n                    style={{ cursor: \"pointer\" }}\r\n                    key={i}\r\n                    onClick={() => {\r\n                      props.history.push(`/web/user/${obj._id}`);\r\n                    }}\r\n                  >\r\n                    <td>{obj.name}</td>\r\n                    <td>{obj.role}</td>\r\n                    <td>{obj.active ? <BsCheck></BsCheck> : <BsX></BsX>}</td>\r\n                    <td>\r\n                      <button\r\n                        className=\"btn btn-sm btn-danger\"\r\n    