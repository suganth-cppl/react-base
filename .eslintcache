Router>\n        </ToastProvider>\n      </div>\n    </>\n  );\n}\n\nexport default App;\n\nconst WebContainer = (props) => {\n  return (\n    <>\n      <Switch>\n        <Route exact path=\"/\" component={Login} />\n        <Route exact path=\"/logout\" component={Logout} />\n        <PrivateRoute path=\"/web\" component={Landing} />\n        <Route path=\"/not-found\" component={NotFound} />\n        <Route exact component={NotFound} />\n      </Switch>\n    </>\n  );\n};\n\nconst MyCustomToast = (props) => {\n  return <CustomToastComponent {...props}></CustomToastComponent>;\n};\n\nconst PrivateRoute = ({ component: Component, authed, ...rest }) => {\n  const user = get_user();\n\n  return (\n    <Route\n      {...rest}\n      render={(props) =>\n        user?.id ? (\n          <Component {...props} />\n        ) : (\n          <Redirect to={{ pathname: \"/\", state: { from: props.location } }} />\n        )\n      }\n    />\n  );\n};\n","D:\\salem\\salem_client\\src\\reportWebVitals.js",[],"D:\\salem\\salem_client\\src\\redux\\reducers\\root_reducer.js",[],"D:\\salem\\salem_client\\src\\components\\toast\\custom-toast.js",[],"D:\\salem\\salem_client\\src\\auth\\login.js",[],"D:\\salem\\salem_client\\src\\pages\\landing.js",[],"D:\\salem\\salem_client\\src\\utils\\axios-utils.js",[],"D:\\salem\\salem_client\\src\\pages\\not-found.js",[],"D:\\salem\\salem_client\\src\\auth\\auth.js",[],"D:\\salem\\salem_client\\src\\components\\header\\index.js",[],"D:\\salem\\salem_client\\src\\pages\\logout.js",["204"],"import React, { useEffect } from \"react\";\r\nimport { remove_user } from \"./../auth/auth\";\r\nimport { FiLogOut } from \"react-icons/fi\";\r\nimport { connect } from \"react-redux\";\r\nimport { user_logout_action } from \"../redux/actions/user_action\";\r\n\r\nconst Logout = (props) => {\r\n  useEffect(() => {\r\n    remove_user();\r\n    props.user_logout_action();\r\n    props.history.push(\"/\");\r\n  }, []);\r\n\r\n  return (\r\n    <>\r\n      <div className=\"container\">\r\n        <div\r\n          className=\"row align-items-center justify-content-center\"\r\n          style={{ height: \"100vh\" }}\r\n        >\r\n          <div className=\"col\">\r\n            <div className=\"w-25 m-auto\">\r\n              <div className=\"form-group text-center\">\r\n                <div>\r\n                  <FiLogOut size={50}></FiLogOut>\r\n                </div>\r\n\r\n                <div className=\"mt-2\">Logging out</div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </>\r\n  );\r\n};\r\n\r\nconst mapDispatchToProps = (dispatch) => {\r\n  return {\r\n    user_logout_action: () => {\r\n      dispatch(user_logout_action());\r\n    },\r\n  };\r\n};\r\n\r\nexport default connect(null, mapDispatchToProps)(Logout);\r\n","D:\\salem\\salem_client\\src\\redux\\reducers\\user_reducer.js",[],"D:\\salem\\salem_client\\src\\redux\\actions\\user_action.js",[],"D:\\salem\\salem_client\\src\\pages\\user.js",["205"],"import React, { useState, useEffect } from \"react\";\r\nimport axios from \"../utils/axios-utils\";\r\nimport moment from \"moment\";\r\nimport { useToasts } from \"react-toast-notifications\";\r\nimport { Link } from \"react-router-dom\";\r\nimport { BiSave } from \"react-icons/bi\";\r\nimport { BsCheck, BsX, BsTrash } from \"react-icons/bs\";\r\n\r\nconst User = (props) => {\r\n  const { addToast } = useToasts();\r\n\r\n  const { id } = props.match.params;\r\n\r\n  const [user, set_user] = useState({\r\n    name: \"\",\r\n    username: \"\",\r\n    role: \"user\",\r\n    active: true,\r\n    password: \"\",\r\n  });\r\n  const [user_list, set_user_list] = useState([]);\r\n  const [refetch, set_refetch] = useState(false);\r\n\r\n  useEffect(() => {\r\n    axios\r\n      .get(`${axios.defaults.baseURL}auth/user-list`)\r\n      .then((response) => {\r\n        set_user_list(response.data.data);\r\n      })\r\n      .catch((error) => {\r\n        console.log(error);\r\n      });\r\n  }, [refetch]);\r\n\r\n  useEffect(async () => {\r\n    if (id && id !== \"new\") {\r\n      try {\r\n        const result = await axios.get(\r\n          `${axios.defaults.baseURL}auth/get-user/${id}`\r\n        );\r\n\r\n        set_user({ ...result.data.data, password: \"\" });\r\n      } catch (e) {\r\n        console.log(e);\r\n      }\r\n    } else {\r\n      set_user({\r\n        name: \"\",\r\n        username: \"\",\r\n        role: \"user\",\r\n        active: true,\r\n        password: \"\",\r\n      });\r\n    }\r\n  }, [id]);\r\n\r\n  const handleSave = () => {\r\n    if (user.name && user.username && user.password) {\r\n      if (user._id) {\r\n        axios\r\n          .put(`${axios.defaults.baseURL}auth/update-user/${user._id}`, user)\r\n          .then((response) => {\r\n            addToast(\"User updated successfully\", {\r\n              appearance: \"success\",\r\n              autoDismiss: true,\r\n            });\r\n            set_refetch(!refetch);\r\n            props.history.push(`/web/user/${response.data.data._id}`);\r\n          })\r\n          .catch((error) => {\r\n            if (error.status === 401) {\r\n              addToast(\"Password Invalid\", {\r\n                appearance: \"error\",\r\n                autoDismiss: true,\r\n              });\r\n            }\r\n\r\n            if (error.status === 404) {\r\n              addToast(\"User Invalid\", {\r\n                appearance: \"error\",\r\n                autoDismiss: true,\r\n              });\r\n            }\r\n            console.log(error);\r\n          });\r\n      } else {\r\n        axios\r\n          .post(`${axios.defaults.baseURL}auth/create-user`, user)\r\n          .then((response) => {\r\n            addToast(\"User saved successfully\", {\r\n              appearance: \"success\",\r\n              autoDismiss: true,\r\n            });\r\n            set_refetch(!refetch);\r\n            props.history.push(`/web/user/${response.dat